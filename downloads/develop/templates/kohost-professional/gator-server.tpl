    <script>
        var categoriesList = new Swiper(".categories-list", {
            grabCursor: true,
            slidesPerView: "auto", //innerWidth > 808 ? "auto" : 1,
            spaceBetween: 20,
            loop: true,
            freeMode: false,
            navigation: {
                nextEl: ".recent-template-categories .swiper-button-next",
                prevEl: ".recent-template-categories .swiper-button-prev",
            },
        });

        var recentCards = new Swiper("#recentCards", {
            grabCursor: true,
            slidesPerView: "auto", //innerWidth > 808 ? "auto" : 1,
            spaceBetween: 50,
            loop: true,
            navigation: {
                nextEl: ".recent-cards-container .swiper-button-next",
                prevEl: ".recent-cards-container .swiper-button-prev",
            },
            centeredSlidesBounds: true,
            centerInsufficientSlides: true,
            centeredSlides: true,
        });

        // ----------------------------------------------------------------------------

        const openMenu = document.querySelector(".open-menu");
        const closeMenu = document.querySelector(".close-menu");
        const mainNav = document.querySelector("nav");

        openMenu.addEventListener("click", (e) => {
            mainNav.style.right = "0";
            closeMenu.style.display = "block";
            openMenu.style.display = "none";
            document.body.style.overflow = "hidden";
        });

        closeMenu.addEventListener("click", (e) => {
            mainNav.style.right = "-100%";
            closeMenu.style.display = "none";
            openMenu.style.display = "block";

            document.body.style.overflow = "auto";
        });

        // ----------------------------------------------------------------------------

        const stickyNav = document.querySelector(".sticky-nav");
        const stickyNavList = document.querySelectorAll(".sticky-nav ul li");

        const cW = document.getElementById("createWebsite");
        const wH = document.getElementById("woocommerceHosting");
        const mH = document.getElementById("magentoHosting");
        const oS = document.getElementById("onlineStore");
        const pE = document.getElementById("professionalEmail");

        let elementsToObserver = [cW, wH, mH, oS, pE];

        const cwObserver = new IntersectionObserver((entry) => {
            if (!entry[0].isIntersecting && window.scrollY > 1000) {
                stickyNav.style.display = "block";
            } else stickyNav.style.display = "none";
        });

        cwObserver.observe(cW);

        const observer = new IntersectionObserver((entries) => {
            let id = entries[0].target.id;

            stickyNavList.forEach((ele) => {
                ele.classList.remove("active");
            });

            if (entries[0].intersectionRatio > 0) {
                switch (id) {
                    case "createWebsite":
                        stickyNavList[0].classList.add("active");

                        break;

                    case "woocommerceHosting":
                        stickyNavList[1].classList.add("active");
                        break;

                    case "magentoHosting":
                        stickyNavList[2].classList.add("active");
                        break;

                    case "onlineStore":
                        stickyNavList[3].classList.add("active");
                        break;

                    case "professionalEmail":
                        stickyNavList[4].classList.add("active");
                        break;

                    default:
                        stickyNavList.forEach((ele) => {
                            ele.classList.remove("active");
                        });

                        break;
                }
            }
        });

        elementsToObserver.forEach((ele) => {
            observer.observe(ele);
        });

        let activeID = null;
        const navList = document.querySelector(".nav-list");

        navList.addEventListener("click", (e) => {
            let li = e.target.closest("li");
            let dataID = li.dataset.id;

            if (activeID === dataID) {
                li.classList.remove("active");
                activeID = null;
            } else {
                const list = navList.querySelectorAll("li[data-id]");

                list.forEach((ele) => {
                    ele.classList.remove("active");
                });

                if (dataID) {
                    li.classList.add("active");
                    activeID = dataID;
                }
            }
        });
    </script>
